package com.balaji.modal;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="transactions")
public class Transaction {
	
	private int tamount;
	private int idrecv;
	private boolean tstatus;
	@Id
	private int tid;
	private int id;
	
	public Transaction() {
		
	}
	
	public Transaction(int tamount, int idrecv, boolean tstatus, int id) {
		super();
		this.tamount = tamount;
		this.idrecv = idrecv;
		this.tstatus = tstatus;
		this.id = id;
	}
	public int getTamount() {
		return tamount;
	}
	public void setTamount(int tamount) {
		this.tamount = tamount;
	}
	public int getIdrecv() {
		return idrecv;
	}
	public void setIdrecv(int idrecv) {
		this.idrecv = idrecv;
	}
	public int getTstatus() {
		return tstatus;
	}
	public void setTstatus(int tstaus) {
		this.tstatus = tstatus;
	}
	public int getTid() {
		return tid;
	}
	public void setTid(int tid) {
		this.tid = tid;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	@Override
	public String toString() {
		return "Transaction [tamount=" + tamount + ", idrecv=" + idrecv + ", tstatus=" + tstaus + ", tid=" + tid
				+ ", id=" + id + "]";
	}
	
	
	
	
	
	
	
	
	
}
